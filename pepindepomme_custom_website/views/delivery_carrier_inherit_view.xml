<odoo>
	<data>
		<record id="delivery_carrier_form_view_inherit" model="ir.ui.view">
			<field name="name">delivery.carrier.icon.form</field>
			<field name="model">delivery.carrier</field>
			<field name="inherit_id" ref="delivery.view_delivery_carrier_form"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='zip_from']" position="after">
					<field name="icon" widget="image" class="oe_avatar"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>



