<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="choose_delivery_carrier_view_form" model="ir.ui.view">
        <field name="name">choose.delivery.carrier.form</field>
        <field name="model">choose.delivery.carrier</field>
        <field name="inherit_id" ref="delivery.choose_delivery_carrier_view_form"/>
        <field name="arch" type="xml">
            <form position="inside">
                <field name="is_colissimo" invisible="1"/>
                <div class="o_zone_widget" attrs="{'invisible': [('is_colissimo', '=', False)]}"/>
                <input type="hidden" id="pudoWidgetErrorCode"  attrs="{'invisible': [('is_colissimo', '=', False)]}" />
                <field name="colissimo_last_selected" widget="colissimo_relay"/>
                <field name="colissimo_last_selected_id" invisible="1"/>
                <field name="colissimo_brand" invisible="1"/>
                <field name="colissimo_colLivMod" invisible="1"/>
                <field name="colissimo_allowed_countries" invisible="1"/>
                <field name="shipping_zip" invisible="1"/>
                <field name="shipping_country_code" invisible="1"/>
                   <field name="colissimo_ceAddress" invisible="1"/>
                   <field name="colissimo_ceTown" invisible="1"/>
                 <field name="colissimo_ceToken" invisible="1"/>
                <field name="data_holder" invisible="0"/>

            </form>
        </field>
    </record>
</odoo>
